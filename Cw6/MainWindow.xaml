<Window x:Class="Cw6.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Cw6"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="Assets" Content="See Assets" Margin="10,10,678,363" Click="Assets_Click"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Margin="10,97,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="112" TextChanged="textBox_TextChanged"/>
        <DataGrid x:Name="textBoxes" Margin="0,140,0,-140" SelectionChanged="DataGrid_SelectionChanged"/>

    </Grid>
</Window>
